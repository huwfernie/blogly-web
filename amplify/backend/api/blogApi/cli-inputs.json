{
  "version": 1,
  "paths": {
    "/blog/{blogId}": {
      "permissions": {
        "setting": "open"
      },
      "lambdaFunction": "blogLambda"
    },
    "/users/{userId}": {
      "name": "/users/{userId}",
      "lambdaFunction": "blogUsersLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    },
    "/author/{id}": {
      "name": "/author/{id}",
      "lambdaFunction": "bloglyWebAuthorLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "read"
        ]
      }
    }
  }
}